{
  "name": "dcr/framework",
  "description": "another framework",
  "type": "project",
  "keywords": [
    "framework",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://symfony.com",
  "authors": [
    {
      "name": "guanhui07",
      "email": "guanhui07@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1.0",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-pcntl": "*",
    "ext-redis": "*",
    "ext-simplexml": "*",
    "ext-swoole": "*",
    "doctrine/annotations": "^2.0",
    "doctrine/event-manager": "^1.1",
    "doctrine/instantiator": "^1.5",
    "doctrine/migrations": "^3.5",
    "elasticsearch/elasticsearch": "7.16",
    "firebase/php-jwt": "^6.3",
    "gregwar/captcha": "^1.1",
    "guanhui07/facade": "^1.0",
    "guzzlehttp/guzzle": "^7.5",
    "illuminate/database": "^9.45",
    "illuminate/support": "^9.0",
    "inhere/console": "^4.1",
    "inhere/php-validate": "^3.0",
    "intervention/image": "^2.7",
    "joanhey/adapterman": "^0.5.5",
    "middlewares/utils": "^3.3",
    "monolog/monolog": "^3.2",
    "mwangithegreat/faker": "^1.9",
    "nesbot/carbon": "^2.64",
    "nikic/fast-route": "^1.3",
    "opis/closure": "^3.6",
    "php-amqplib/php-amqplib": "^3.0",
    "php-di/php-di": "^7.0",
    "predis/predis": "^2.0",
    "qiniu/php-sdk": "^7.7",
    "spatie/image": "^2.2",
    "symfony/event-dispatcher": "^6.2",
    "symfony/finder": "^6.2",
    "symfony/http-kernel": "^6.2",
    "vlucas/phpdotenv": "^5.4",
    "workerman/crontab": "^1.0",
    "workerman/gateway-worker": "^3.0.0",
    "workerman/gatewayclient": "^3.0",
    "workerman/workerman": "^3.5"
  },
  "require-dev": {
    "phpunit/php-timer": "^5.0",
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "roave/security-advisories": "dev-latest",
    "swoole/ide-helper": "~5.0.0"
  },
  "suggest": {
    "php": ">=8.2",
    "ext-event": "For better performance. ",
    "ext-swoole": "Swoole Server is Fasted plugin"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/",
      "dcr\\": "dcr/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DcrTest\\": "./test/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "cp -r -f ./fix/AbstractHandler.php ./vendor/inhere/console/src/Handler"
    ],
    "serve": [
      "@php -S 127.0.0.1:8001 -t ./public"
    ],
    "start": [
      "@php -S 127.0.0.1:8001 -t ./public"
    ],
    "ws": "@php ./bin/startWs.php start ",
    "crontab": "@php ./bin/crontab.php start",
    "test": "./vendor/bin/phpunit -c phpunit.xml --colors=always",
    "cs-fix": "php-cs-fixer fix $1",
    "phpstan": "./vendor/bin/phpstan analyze"
  },
  "config": {
    "secure-http": false,
    "optimize-autoloader": true,
    "platform-check": false,
    "sort-packages": true
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
